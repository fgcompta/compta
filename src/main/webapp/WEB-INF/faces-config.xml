<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<application>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
<!-- 	<converter> -->
<!-- 		<converter-for-class>java.lang.Enum</converter-for-class> -->
<!-- 		<converter-class>compta.client.util.converter.EnumConverter</converter-class> -->
<!-- 	</converter> -->
	<managed-bean>
		<managed-bean-name>statsBean</managed-bean-name>
		<managed-bean-class>compta.client.StatsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>


	<!-- navigation -->
	<navigation-rule>
		<description />
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>articles</from-outcome>
			<to-view-id>/articles/articles.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<description />
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>customers</from-outcome>
			<to-view-id>/customers/customers.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<description />
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>newQuote</from-outcome>
			<to-view-id>/quote/quote.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<description />
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>searchQuote</from-outcome>
			<to-view-id>/quote/searchQuote.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<description />
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>stats</from-outcome>
			<to-view-id>/stats/stats.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>